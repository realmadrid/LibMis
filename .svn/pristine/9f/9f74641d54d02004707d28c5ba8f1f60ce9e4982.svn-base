@CHARSET "UTF-8";
body {
    background: none repeat scroll 0 0 #F1F1F1;
    color: #000000;
    font-size: 12px;
    line-height: 166.6%;
    text-align: center;
}
pagenum{float:inherit
}
body, input, select, button, textarea,sexInp {
    font-family: Verdana,arial,sans-serif;
}
h1, h2, h3, h4, h5, h6, select, input, textarea, button, table {
    font-size: 100%;
}
body, h1, h2, h3, h4, h5, h6, ul, ol, li, form, p, dl, dt, dd, table, th, td, img, blockquote, legend, fieldset {
	border: 0 none;
	padding: 0;
}
input, button, textarea, select, img {
    line-height: normal;
    margin: 0;
}
abbr, acronym, address, cite, q, em, code, var, dfn, ins {
    font-style: normal;
    text-decoration: none;
}
q:before, q:after {
    content: "";
}
ul, ol {
    list-style: none outside none;
}
#table {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	font-style: normal;
	line-height: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	margin-right: 25px;
	margin-bottom: 25px;
	margin-left: 25px;
	text-align: center;
	display: inline;
}
.ipt-b, button {
    line-height: 135%;
    overflow: visible;
    padding: 0 0.5em;
}
select, input, button, button img {
    vertical-align: middle;
}
.ipt-r, .ipt-c {
    height: 16px;
    margin: 2px 3px 2px 0;
    overflow: hidden;
    padding: 0;
    width: 16px;
}
.page, .header, .content, .footer {
    margin: auto;
}
.fn-noime {
    ime-mode: disabled;
}
.fn-hide {
    display: none;
}
.ico, .fn-ib, .collection-main .notice {
    display: inline-block;
    vertical-align: middle;
}
a {
    color: #1D50A2;
    text-decoration: none;
}
a:hover {
    text-decoration: underline;
}
.txt-err, .txt-impt {
    color: #FF0000;
}
.txt-info {
    color: #999999;
}
.txt-succ {
    color: #0E931C;
}
.txt-infoNotice {
    color: #606060;
}
#Rheader .slogan, #Rbody .title .crl, #Rbody .title .crr, .ico, .btn-reg, .btn-enter, .btn-ok, #Rbody .bottom .crl, #Rbody .bottom .crr, .Succ-msg .split {
    background-image: url("../images/global.png");
    background-repeat: no-repeat;
}
#Rbody .title, .g-ipt, .g-ipt-active, .g-ipt-err, #Rbody .bottom, .Succ-msg-info .ct {
    background-image: url("../img/bgx.png");
    background-repeat: repeat-x;
}
.g-collection {
}
.g-collection-item {
    padding-bottom: 10px;
    padding-left: 340px;
    position: relative;
}
.g-collection-label {
    display: block;
    left: 0;
    position: absolute;
    text-align: right;
    top: 9px;
    width: 335px;
}
.g-ipt {
    background-color: #FBFBFB;
    background-position: 0 -72px;
    border: 1px solid #C1C1C1;
    color: #444444;
    font-size: 14px;
    padding: 4px 5px;
}
.g-ipt-active {
    background-color: #FFFFCC;
    background-position: 0 -108px;
    border-color: #C6C69E;
}
.g-ipt-err {
    background-color: #FBE2E2;
    background-position: 0 -144px;
    border-color: #D28C8C;
    color: #A63B3B;
}
.g-oipt {
    font-size: 12px;
    padding: 3px 0 2px;
    width: 160px;
}
.ico-warning, .ico-succ {
    height: 24px;
    width: 24px;
}
.ico-warning {
    background-position: -252px -72px;
}
.ico-succ {
    background-position: -180px -72px;
}
.ico-succ-big {
    background-position: -322px -70px;
    height: 52px;
    width: 52px;
}
.ico-download {
    background-position: -251px -108px;
    height: 10px;
    width: 12px;
}
.ico-free {
    background-position: -180px -144px;
    height: 17px;
    width: 55px;
}
.btn-reg {
    background-color: transparent;
    background-position: 0 -179px;
    border: 0 none;
    cursor: pointer;
    height: 38px;
    margin-top: 15px;
    width: 104px;
}
.btn-enter {
    background-color: transparent;
    background-position: -144px -179px;
    border: 0 none;
    cursor: pointer;
    height: 38px;
    width: 162px;
}
.btn-ok {
    background-color: transparent;
    background-position: 0 -288px;
    border: 0 none;
    cursor: pointer;
    height: 39px;
    margin: 10px 20px 10px 0;
    vertical-align: middle;
    width: 76px;
}
#Rpage {
    margin: 0 auto;
    text-align: left;
    width: 1000px;
}
#Rheader {
    font-size: 14px;
    height: 80px;
    overflow: hidden;
    position: relative;
    width: 100%;
}
#Rheader .logo {
    display: block;
    left: 0;
    position: absolute;
    top: 24px;
}
#Rheader .slogan {
    display: block;
    height: 33px;
    left: 140px;
    overflow: hidden;
    position: absolute;
    text-indent: 800px;
    top: 27px;
    white-space: nowrap;
    width: 306px;
}
#Rheader .ext {
    line-height: 100px;
    text-align: right;
    white-space: nowrap;
}
#Rbody {
    font-size: 14px;
    height: auto;
    width: 100%;
}
#Rbody .title {
    background-color: #458DC8;
    background-position: 0 0;
    height: 46px;
    position: relative;
}
#Rbody .title .ext {
    color: #FFFFFF;
    float: right;
    line-height: 46px;
    margin-right: 10px;
    white-space: nowrap;
}
#Rbody .title .crl, #Rbody .title .crr {
    display: block;
    height: 46px;
    position: absolute;
    top: 0;
}
#Rbody .title .crl {
	background-position: -324px 0;
	height: 46px;
	left: 0px;
	width: 42px;
}
#Rbody .title .crr {
    background-position: -366px 0;
    height: 46px;
    right: 0;
    width: 4px;
}
#Rbody .title h1 {
    color: #FFFFFF;
    font-size: 14px;
    font-weight: normal;
    line-height: 46px;
    text-indent: 60px;
}
#Rbody .content {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: #FFFFFF;
    border-color: #DCDCDC;
    border-style: solid;
    border-width: 0 1px;
    overflow: hidden;
}
.collection-main {
	margin:0px;
	background-color: #FFF;
	background-image: url(images/bg.gif);
	background-repeat: repeat;
}
.collection-main .ipt-wraper {
    display: block;
    margin-bottom: 3px;
    margin-left: -5px;
    padding: 5px;
    width: 570px;
}
.collection-main .ipt-wraper-active {
    background-color: #F4F4F4;
}
.collection-main .g-ipt {
    width: 300px;
}
.collection-main .notice {
    margin-left: 10px;
}
.collection-main .notice .ico {
    margin-right: 8px;
}
.collection-main .pswState .ico {
    height: 19px;
    margin: 0 8px;
    width: 147px;
}
.collection-main .pswState-empty .ico {
    background-position: 0 -36px;
}
.collection-main .pswState-poor .ico {
    background-position: 0 -72px;
}
.collection-main .pswState-normal .ico {
    background-position: 0 -108px;
}
.collection-main .pswState-strong .ico {
    background-position: 0 -144px;
}
.collection-main .pswState .st1 {
    color: #FF0000;
}
.collection-main .pswState .st2 {
    color: #13A821;
}
.collection-main .txt-info {
    font-size: 12px;
    margin-top: -4px;
}
.collection-main .g-select {
    color: #555555;
    font-size: 14px;
    vertical-align: middle;
}
.collection-main .vcode_img {
    border: 1px solid #E7E7E7;
    cursor: pointer;
    vertical-align: middle;
}
.chose-list {
    background-color: #FFFFFF;
    border: 1px solid #C6C6A8;
    height: auto;
    margin-top: 10px;
    overflow: visible;
    position: relative;
    width: 306px;
}
div.chose-list-onlytitle {
    padding-bottom: 0;
}
div.chose-list-onlytitle .tit {
    border-bottom: 0 none;
    margin-bottom: 0;
}
div.chose-list-onlytitle .tit .goOn {
    color: #099E3A;
    font-weight: bold;
}
div.chose-list-onlytitle .recommend, div.chose-list-onlytitle ul {
    display: none;
}
.chose-list .arr {
    background-position: -324px -144px;
    display: block;
    height: 10px;
    left: 50px;
    overflow: hidden;
    position: absolute;
    top: -9px;
    width: 17px;
}
.chose-list .tit {
    background-color: #FFFFE1;
    border-bottom: 1px solid #DCDCCA;
    border-style: solid;
    border-top: 1px solid #FFFFFF;
    border-width: 1px 0;
    color: #1C1C1C;
    font-size: 14px;
    height: 31px;
    line-height: 31px;
    margin-bottom: 1px;
    text-indent: 12px;
}
.chose-list li {
    font-size: 12px;
    height: 23px;
    padding: 2px 8px;
    position: relative;
}
.chose-list .stat {
    position: absolute;
    right: 5px;
    top: 2px;
    white-space: nowrap;
}
.chose-list label {
    cursor: pointer;
    display: block;
    float: left;
    overflow: hidden;
    white-space: nowrap;
    width: 220px;
}
.chose-list label em {
    color: #099E00;
    font-weight: bold;
}
.chose-list li .stat {
    color: #099E00;
}
.chose-list li.disabled, .chose-list li.disabled em, .chose-list li.disabled .stat {
    color: #999999;
    font-weight: normal;
}
.chose-list .recommend {
    border-top: 1px dotted #C4C4C4;
    padding-bottom: 1px;
}
.chose-list .recommend li {
    height: 22px;
}
.chose-list .recommend li strong {
    color: #666666;
}
.chose-list .recommend .stat {
    color: #000000;
}
.chose-list ul {
    padding: 5px 0 1px;
}
#Rbody .bottom {
    background-position: 0 -180px;
    height: 8px;
    overflow: hidden;
    position: relative;
}
#Rbody .bottom .crl, #Rbody .bottom .crr {
    display: block;
    height: 8px;
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 9px;
}
#Rbody .bottom .crl {
    background-position: -252px -144px;
    left: 0;
}
#Rbody .bottom .crr {
    background-position: -261px -144px;
    right: 0;
}
#Rfooter {
    clear: both;
    color: #666666;
    line-height: 26px;
    padding: 10px 0;
    text-align: center;
}
#Rfooter a {
    color: #666666;
}
.Succ-msg {
    background-color: #FFFEF0;
    border: 1px solid #E8E7DA;
    border-radius: 8px 8px 8px 8px;
    height: auto;
    margin-left: 40px;
    width: 406px;
}
.Succ-msg-wel {
    padding: 25px 0 15px 84px;
    position: relative;
    text-align: left;
}
.Succ-msg-wel .ico-succ-big {
    left: 20px;
    position: absolute;
    top: 20px;
}
.Succ-msg-wel .ico-succ-big h2 {
    font-size: 14px;
    font-weight: bold;
}
.Succ-msg-wel .acct {
    color: #1D50A2;
    display: block;
    font-weight: bold;
    height: 30px;
    line-height: 30px;
    overflow: hidden;
    width: 100%;
}
.Succ-msg .split {
    background-position: 0 -252px;
    font-size: 0;
    height: 1px;
    margin: 0 auto;
    overflow: hidden;
    width: 376px;
}
.Succ-msg-info {
    font-size: 14px;
    line-height: 24px;
    padding: 18px 0 18px 30px;
}
.Succ-msg-info .tit {
    color: #999999;
    padding: 3px 0;
}
.Succ-msg-info .ct {
    background-color: #F5F4E7;
    background-position: 0 -288px;
    color: #5B5B5B;
    margin-bottom: 10px;
    padding: 15px 18px;
    width: 310px;
}
.Succ-msg-info .ct dt {
    float: left;
    height: 24px;
    text-align: right;
    white-space: nowrap;
    width: 100px;
}
.Succ-msg-info .ct dd {
    overflow: hidden;
    word-wrap: break-word;
}
.Succ-msg-fun {
    color: #5B5B5B;
    font-size: 12px;
    padding: 10px 20px 20px 45px;
}
.Succ-msg-fun li {
    position: relative;
}
.Succ-msg-fun li .dot {
    font-family: simsun;
    left: -15px;
    position: absolute;
    top: 0;
}
.Succ-mob {
    float: right;
    height: auto;
    overflow: hidden;
    padding-top: 10px;
    text-align: left;
    width: 460px;
}
.Succ-mob .t1 {
    font-weight: bold;
    line-height: 22px;
}
.Succ-mob .t2 {
    color: #999999;
    line-height: 22px;
    padding-bottom: 15px;
}
.Succ-mob .g-ipt {
    width: 150px;
}
.Succ-mob .g-collection-item {
    padding-left: 90px;
}
.Succ-mob .g-collection-label {
    text-align: left;
    width: 90px;
}
.Succ-mob .g-oipt {
    margin: 8px 0;
}
.Succ-mob .btn-enter {
    margin-bottom: 20px;
}
.Succ-mob .txt-err, .Succ-mob .txt-succ {
    padding-right: 90px;
}
.Succ-mob .ipt-wraper {
    width: 100%;
}
.Rtips-Cont {
    font-size: 14px;
    text-align: center;
}
.g-dialogBox-hd, .g-dialogBox-bd .ico-info, .g-dialogBox-ft input, .g-dialogBox-ft a {
    background-image: url("../img/dialog.png");
    background-repeat: no-repeat;
}
.g-dialogBox-mask {
    background: none repeat scroll 0 0 #000000;
    height: 100%;
    left: 0;
    opacity: 0.4;
    position: absolute;
    top: 0;
    width: 100%;
}
.g-dialogBox {
    height: auto;
    left: 50%;
    margin-left: -230px;
    position: absolute;
    text-align: left;
    top: 20%;
    width: 460px;
    z-index: 500;
}
.g-dialogBox-hd {
    background-position: 0 50%;
    height: 27px;
    position: relative;
    text-align: left;
    width: 100%;
}
.g-dialogBox-hd h3 {
    color: #FFFFFF;
    font-size: 14px;
    line-height: 27px;
    text-indent: 20px;
}
.g-dialogBox-hd-oprt-close {
    background-color: #CCCCCC;
    background-position: -438px -7px;
    display: block;
    height: 12px;
    opacity: 0;
    overflow: hidden;
    position: absolute;
    right: 10px;
    text-indent: 20px;
    top: 7px;
    white-space: nowrap;
    width: 12px;
}
.g-dialogBox-bd {
    background-color: #FFFFFF;
    border-color: #6C92AD;
    border-style: solid;
    border-width: 0 1px;
    text-align: left;
}
.g-dialogBox-bd .ico-info {
    background-position: 0 -72px;
    display: block;
    height: 32px;
    left: 28px;
    position: absolute;
    top: 18px;
    width: 32px;
}
.g-dialogBox-iconText {
    padding: 23px 10px 20px 70px;
    position: relative;
    text-align: left;
}
.g-dialogBox-ft {
    background-color: #F6F9FC;
    border: 1px solid #6C92AD;
    padding: 8px 15px;
    text-align: right;
}
.g-dialogBox-ft input {
    background-position: 0 -36px;
    border-width: 0;
    cursor: pointer;
    height: 24px;
    margin-left: 10px;
    width: 52px;
}
.g-dialogBox-ft a {
    background-position: 0 -36px;
    border-width: 0;
    color: #000000;
    cursor: pointer;
    display: inline-block;
    height: 24px;
    line-height: 24px;
    margin-left: 10px;
    text-align: center;
    text-decoration: none;
    vertical-align: middle;
    width: 52px;
}
.Rpage-main .g-ipt {
    padding: 4px;
}
.Rpage-main .g-collection-label {
    top: 9px;
}
.Rpage-main .g-collection-label .txt-impt {
    position: relative;
    top: 2px;
}
.Rpage-main .collection-main .ipt-wraper {
    position: relative;
}
.Rpage-main .collection-main .ipt-wraper .notice {
    left: 380px;
    overflow: visible;
    position: absolute;
    top: 6px;
}
.Rpage-main .collection-main .ipt-wraper .pswState {
    top: 8px;
}
.Rpage-main .collection-main .notice .ico {
    padding: 2px 0;
}
.Rpage-main .collection-main {
    padding: 43px 0 35px;
}
.Rpage-main .provision {
    font-size: 12px;
}
.chose-list .recommend a {
    color: #000000;
    text-decoration: none;
}
.chose-list .recommend a:hover {
    text-decoration: underline;
}
.chose-list .recommend label {
    cursor: default;
}
.noscript {
    background-color: #FFFFE1;
    border: 1px solid #C9C9AB;
    border-radius: 5px 5px 5px 5px;
    font-size: 14px;
    margin: 0 auto 30px;
    padding: 15px 0;
    text-align: center;
    width: 520px;
}
#vcode_img{
	margin:20px;
}